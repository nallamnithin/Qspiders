
class  LoopOddNumber
{
	public static void main(String[] args) 
	{
		for (int i =20;i>=1;i--)
		{
			if (i%2!=0)
			{
				System.out.println(i);
		  }
	   }
	   System.out.println("==============================================");
    
	for (int i=19;i>=1 ;i=i-2)
		System.out.println(i);
	}
 }
